<main>
  <div class="return-home text-center"><a class="color-white" href="https://www.nba.com/timberwolves/">Return to Timberwolves.com</a></div>
  <header class="header">
    <div class="size-md scale" title="One Pack: We're all in the together. Brought to you by the Minnesota Timberwolves, Minnesota Lynx, Iowa Wolves, and T-Wolves Gaming.">
      <div class="border-8 border-aurora">
        <h1 class="color-white mt-5 text-7xl text-center">Know Before You Go</h1>
        <h3 class="color-white text-center">Steps we're taking as we welcome fans back to Target Center.</h3>
      </div>

    </div>
  </header>
  <nav class="nav" 
    x-data="nav()"
    x-spread="scroll"
    >
    <ul class="nav__menu">
      <template x-for="item in navitems" :key="item.id">
        <li class="nav__item">
          <a class="nav__link font-bold"
            :class="{ '--active' : atSection===item.id }"  
            x-bind:href="`#${item.id}`"
            @click.prevent="atSection = item.id; smoothscroll(item, $el)">
            <span x-text="item.title"></span>
          </a>
        </li>
      </template>

    </ul>
  </nav>
  <section class="section bg-white color-midnight" id="gameday" x-data="gameday()" x-init="modal = gameday[0];" @keydown.escape.window="showModal = false" x-cloak>
    <div class="size-lg margin-auto">
      <div class="text-center">
        <h2>What to Expect on Game Day</h2>
      </div>
      <div class="card-container">
        <template x-for="item in gameday" :key="item.title">
          <div class="card">
            <div class="card__image">
              <i 
                x-bind:class="`${item.icon}`"
                aria-hidden="true"></i>
            </div>
            <div class="card__title" x-text="item.title"></div>
            <p class="card__description" x-html="item.note"></p>
            <template x-if="item.cta && !item.modal">
              <a 
                class="card__button" 
                x-bind:href="item.link"
                rel="'noopener noreferrer'"
                target="__blank"
                x-text="item.cta"></a>
            </template>
            <template x-if="item.cta && item.modal">
              <a 
              class="card__button" 
              nohref
              @click="$dispatch('modal', item)" x-text="item.cta"></a>
            </template>
          </div>
        </template>
      </div>
    </div>
    <div class="overflow-auto fixed inset-0 z-10 flex items-center justify-center" style="background-color: rgba(0,0,0,0.5)" @modal.window="console.log('dispatched'), showModal = true, modal = $event.detail" x-show="showModal">
      <!--Dialog-->
      <div class="bg-white w-11/12 md:max-w-xl mx-auto rounded shadow-lg py-4 text-left px-6" x-show="showModal" @click.away="showModal = false" x-transition:enter="ease-out duration-300" x-transition:enter-start="opacity-0 mt-16" x-transition:enter-end="opacity-100 mt-0" x-transition:leave="ease-in duration-150" x-transition:leave-start="opacity-100" x-transition:leave-end="opacity-0">
          <!--Title-->
            <div class="flex justify-between items-center pb-3">
              <p class="text-2xl font-bold" x-text="modal.title"></p>
              <div class="cursor-pointer z-50" @click="showModal = false">
                <svg class="fill-current text-black" xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 18 18">
                  <path d="M14.53 4.53l-1.06-1.06L9 7.94 4.53 3.47 3.47 4.53 7.94 9l-4.47 4.47 1.06 1.06L9 10.06l4.47 4.47 1.06-1.06L10.06 9z"></path>
                </svg>
              </div>
            </div>

            <!-- content -->
            <p x-html="modal.modalInfo"></p>

            <!--Footer-->
            <div class="flex justify-end pt-2">
              <button class="modal-close px-4 bg-lake-light p-3 rounded-lg text-lake hover:bg-lake-light-interact" @click="showModal = false">Close</button>
            </div>

      </div>
      <!--/Dialog -->
    </div><!-- /Overlay -->
  </section>
  <section class="section bg-lake text-white" id="mayo">
    <div class="resource size-lg margin-auto md:hidden">
      <div class="resource__logo lined">
