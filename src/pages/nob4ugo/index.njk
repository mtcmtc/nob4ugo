<script type="text/javascript">
  function getParameterByName(name, url = window.location.href) {
    name = name.replace(/[\[\]]/g, '\\$&');
    var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
        results = regex.exec(url);
    if (!results) return null;
    if (!results[2]) return '';
    return decodeURIComponent(results[2].replace(/\+/g, ' '));
  }

  function nav() {
    return{
      atSection : '',
      navitems : [
        {
          id : `gameday`,
          title: `What to Expect`,
        },
        {
          id : `mayo`,
          title: `Mayo Clinic Guidelines`,
        },
        // {
        //   id : `faq`,
        //   title: `FAQ`,
        // },
        {
          id : `moreinfo`,
          title: `More Info`

        },
      ],
      scroll: {
        ['@scroll.window']() { 
          this.atSection = 
            document.querySelector('#moreinfo').getBoundingClientRect().top <= 100 ? 'moreinfo' 
              : (document.querySelector('#mayo').getBoundingClientRect().top <= 100 ? 'mayo' 
              : 'gameday')
        },
      },
      smoothscroll(item, component) {
        const toScroll = window.pageYOffset + document.querySelector(`#${item.id}`).getBoundingClientRect().top - component.offsetHeight;

        window.scroll({
          top: toScroll,
          behavior: 'smooth'
        });
        
        // console.log(`scrolling to ${item.id}`); 
      }
    }
  }

